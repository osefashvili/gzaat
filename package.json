{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Webpack boilerplate for front-end",
  "main": "index.js",
  "scripts": {
    "build":
      "NODE_ENV=production ./node_modules/.bin/webpack --progress --profile --colors",
    "start": "./node_modules/.bin/webpack -w",
    "serve": "node server.js"
  },
  "keywords": ["webpack", "boilerplate", "sass", "es6", "eslint"],
  "author": "David Babunashvili",
  "license": "ISC",
  "browserslist": ["> 1%", "ie > 9"],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0-beta.36",
    "autoprefixer": "^7.2.4",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.0-beta.0",
    "browser-sync-webpack-plugin": "^1.2.0",
    "connect": "^3.6.5",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "serve-static": "^1.13.1",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "browser-sync": "^2.23.3"
  }
}
